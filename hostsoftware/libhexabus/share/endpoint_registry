; This file contains a registry of endpoints.
;
; The registry constists of any number of eid descriptors, where every eid descriptor is of the format
;
; eid <eid#> {
;   type <type>
;   description "<description>"
;   unit "<unit>"                 ; optional
;   access <access>
; }
;
; where:
;  * <eid#> is any valid EID number
;  * <description> is some string
;  * <unit> is some string
;  * <access> is one of
;     * R - read-only
;     * W - write-only
;     * RW - read-write
;  * <type> is one of
;     * BOOL
;     * UINT8
;     * UINT32
;     * DATETIME
;     * FLOAT
;     * 128STRING
;     * TIMESTAMP
;     * 66BYTES
;     * 16BYTES


eid 1 {
	type BOOL
	description "HexabusPlug relay"
	access RW
}

eid 2 {
	type UINT32
	description "HexabusPlug+ power meter"
	unit "W"
	access R
}

eid 3 {
	type FLOAT
	description "Temperature sensor"
	unit "degC"
	access R
}

